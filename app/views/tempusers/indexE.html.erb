<div class="table-responsive">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th class="admin_index"><strong>name</strong></th>
        <th class="admin_index"><strong>major</strong></th>
        <th class="admin_index"><strong>stuN</strong></th>
        <th class="admin_index"><strong>identity</strong></th>
        <th class="admin_index"><strong>password</strong></th>
      </tr>
    </thead>
    <tbody>
      <% @tempusers.each do |i| %>
        <tr>
          <td>
            <%= i.name %>
          </td>
          <td>
            <%= i.major %>
          </td>
            <td>
              <%= i.stuN %>
            </td>
          <td>
            <% if i.identity == "1" %>
              1
            <% elsif i.identity == "2" %>
              2
            <% elsif i.identity == "3" %>
              3
            <% else %>
              admin
            <% end %>
          </td>
          <td>
            <%= i.stuN %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>
</div>
