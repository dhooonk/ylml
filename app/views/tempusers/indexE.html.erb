  <div class="container container-fluid">
    <!-- Page Heading -->
      <div class="row">
        <div class="col-lg-12">
          <h1 class="page-header">
              가입신청 확인<small>Subscription Status</small>
          </h1>

          <div class="container">
            <a href="/tempusers/index" type="button" class="btn btn-default"> 기본 양식 </a>
