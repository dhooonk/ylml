<%= image_tag("logo.gif", id:"img_session") %>
<br>
<div class="container contents-center">



<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h2><%= t(".sign_up") %> (개발용)</h2>
  <div class="form-inputs">
    <%= f.input :name, required: true, autofocus: true, label: "이름" %>
    <%= f.input :email, required: true %>
    <%= f.input :password, required: true %>
    <%= f.input :password_confirmation, required: true %>
    <%= f.input :stuN, required: true, label: "학번", placeholder: '2013101234'%>
    <%= f.input :admin, required: true, label: "관리자계정", hint: '개발을 목적으로 만든 란이니, 최종 디플로이 시 삭제요망'%>
  </div>
</div>
</div>

  <div class="row">
    <div class="col-md-4 col-md-offset-4">
  <div class="form-actions">
    <%= f.button :submit, t(".sign_up") %>
  </div>

<% end %>

<%= render "devise/shared/links" %>
</div>
  </div>
</div>
